{
  "name": "StreamDeck-Meet",
  "version": "0.6",
  "description": "StreamDeck helper for Google Meet",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": [
        "HueLights.js",
        "StreamDeck.js",
        "MeetWrapper.js",
        "main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "ico-svg/*"
  ]
}
